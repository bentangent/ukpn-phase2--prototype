@html.extend('layout', function(model) {
  @html.block('content', function(model) {
    @model.components.forEach(function(component) {
      @html.include('/ss-calculators/partials/component.vash', component)
    })
  })
  @html.append('styles', function(model) {
    <link rel='stylesheet' href='/ss-calculators/css/ss-calculators.css' />
  })
  @html.append('scripts', function() {
    <script src="/ss-calculators/js/ss-calculators.bundle.js"></script>
  }) 
})
