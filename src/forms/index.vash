@html.extend('layout', function(model) {
  @html.block('content', function(model) {
    @model.components.forEach(function(component) {
      @html.include('/forms/partials/component.vash', component)
    })
  })
  @html.append('styles', function(model) {
    <link rel='stylesheet' href='/forms/css/forms.css' />
  })
  @html.append('scripts', function() {
    <script src="/forms/js/forms.bundle.js"></script>
  }) 
})
